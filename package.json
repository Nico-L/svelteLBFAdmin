{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "build": "NODE_ENV='production' npm-run-all --parallel build:css build:js",
    "build:css": "postcss src/css/tailwind.css -o public/css/styles.css",
    "build:js": "rollup -c",
    "watch:svelte": "rollup -c -w",
    "dev": "npm-run-all --parallel watch:css watch:svelte",
    "start": "sirv public --single",
    "watch:css": "postcss src/css/tailwind.css -o public/css/styles.css --watch"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.3.1",
    "autoprefixer": "^9.7.5",
    "connect-history-api-fallback": "^1.6.0",
    "cssnano": "^4.1.10",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "postcss-import": "^12.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^1.20.0",
    "rollup-plugin-browsersync": "^1.1.0",
    "rollup-plugin-css-only": "^2.0.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-postcss": "^2.5.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fullcalendar/core": "5.0.0-beta.2",
    "@fullcalendar/daygrid": "5.0.0-beta.2",
    "@fullcalendar/interaction": "5.0.0-beta.2",
    "@fullcalendar/timegrid": "5.0.0-beta.2",
    "filepond": "^4.13.0",
    "pell": "^1.0.6",
    "sirv-cli": "^0.4.4",
    "svelte": "^3.20.1",
    "svelte-calendar": "^1.5.0",
    "svelte-dropzone": "^1.0.6",
    "svelte-fa": "^2.1.1",
    "svelte-input-mask": "^0.0.3",
    "svelte-loadable": "^1.4.0",
    "svelte-routing": "^1.4.2",
    "tailwindcss": "^1.2.0"
  }
}
